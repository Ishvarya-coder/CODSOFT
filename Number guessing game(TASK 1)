#include <iostream>
using namespace std;
int main() {
 int guess,num;
 num=7;
 cout<<"Guess a number between 1 to 10"<<endl;
 cin>>guess;
 if(guess==num){
     cout<<"Hooray!! You got it"<<endl;
 }
if(guess!=num&&guess<num)
{
    cout<<"Oops!!,Your guess is low ,Try higher!!"<<endl;
}
if(guess!=num&&guess>num)
{
    cout<<"Oops!!,Your guess is high ,Try Lower!!"<<endl;
}
}
